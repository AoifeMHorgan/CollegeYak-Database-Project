<Window x:Class="CollegeYak.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CollegeYak"
        mc:Ignorable="d"
        Title="Welcome to College Yak" Height="450" Width="300"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="btnLogIn" Content="Log In" HorizontalAlignment="Left" Margin="66,144,0,0" VerticalAlignment="Top" Width="163" Height="71" Click="btnLogIn_Click" FontSize="16"/>
        <Button x:Name="btnSignIn" Content="Sign In" HorizontalAlignment="Left" Margin="66,273,0,0" VerticalAlignment="Top" Width="163" Height="71" Click="btnSignIn_Click" FontSize="16"/>
        <Rectangle Fill="#FFE31C60" HorizontalAlignment="Left" Height="79" Stroke="Black" VerticalAlignment="Top" Width="292" StrokeThickness="0"/>
        <Label x:Name="label" Content="CollegeYak" HorizontalAlignment="Left" Margin="84,19,0,0" VerticalAlignment="Top" Foreground="#FFFBF2F2" FontWeight="Bold" FontSize="22"/>

    </Grid>
</Window>
